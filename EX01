def convert_dates_to_coconuts(datels):
    tehingud = 0
    papaya = 0
    banana = 0
    coconut = 0
    datels2 = datels
    while datels >= 7:
        tehingud += 1
        datels -= 7
        papaya += 1
    while papaya >= 5:
        tehingud += 1
        papaya -= 5
        banana += 2
    while banana >= 3:
        tehingud += 1
        banana -= 3
        coconut += 1
    print(datels2, "datli eest sai ", coconut, " kookost. Kokku toimus ",
                 tehingud, "tehingut.")
    return tehingud
    
#convert_dates_to_coconuts(1)
#convert_dates_to_coconuts(105)
#convert_dates_to_coconuts(110)
#convert_dates_to_coconuts(112)
